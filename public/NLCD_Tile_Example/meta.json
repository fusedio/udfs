              "tileLayer": {
                "@@type": "TileLayer",
                "minZoom": 0,
                "maxZoom": 19,
                "tileSize": 256,
                "pickable": true
              },
              "rasterLayer": {
                "@@type": "BitmapLayer",
                "pickable": true
              },
              "vectorLayer": {
                "@@type": "GeoJsonLayer",
                "stroked": true,
                "filled": false,
                "pickable": true,
                "lineWidthMinPixels": 1,
