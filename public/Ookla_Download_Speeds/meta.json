            "fused:slug": "Ookla_Download_Speeds",
            "fused:name": "Ookla_Download_Speeds",
            "fused:id": null,
            "fused:defaultViewState": {
              "enable": true,
              "latitude": 25.103577058765545,
              "longitude": 11.903392271218976,
