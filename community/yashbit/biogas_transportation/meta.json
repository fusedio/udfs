{
  "tileLayer": {
    "@@type": "TileLayer",
    "minZoom": 0,
    "maxZoom": 19,
    "tileSize": 256,
    "pickable": true
  },
  "rasterLayer": {
    "@@type": "BitmapLayer",
    "pickable": true
  },
  "vectorLayer": {
    "@@type": "GeoJsonLayer",
    "stroked": true,
    "filled": false,
    "pickable": true,
    "lineWidthMinPixels": 1,
    "pointRadiusMinPixels": 1,
    "getLineColor": {
      "@@function": "hasProp",
      "property": "r",
      "present": "@@=[properties.r, properties.g, properties.b]",
      "absent": [
        255,
        0,
        255
      ]
    },
    "getFillColor": [
      255,
      0,
      0,
      40
    ]
  }
}